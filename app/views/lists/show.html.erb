<br>
<%= link_to "Back to lists", lists_path %>
<%= link_to "Add bookmark", new_list_bookmark_path(@list) %>

<ul>
  <% @list.movies.each do |list| %>
    <%= list.title %>
    <%= list.overview %>
    <%= image_tag list.poster_url %>
    <%= list.rating %>
  <% end %>
  <br>
  <% @list.bookmarks.each do |list| %>
    <%= list.comment %>
    <%= link_to "Delete", bookmark_path(list), method: :delete, data: {confirm: 'Are you sure?'} %>
  <% end %>
</ul>
